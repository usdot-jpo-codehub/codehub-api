{
  "parser": "babel-eslint",
  "rules": {
    "no-alert": 0,
    "import/prefer-default-export" : 0,
    "import/no-unresolved" : 0,
    "import/no-extraneous-dependencies" : 0,
    "max-len" : 0,
    "no-param-reassign" : 0,
    "no-unused-vars" : 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0,
    "strict": 0,
    "func-names": 0,
    "new-cap": 0,
    "no-console" : 0,
    "comma-dangle" : 0,
    "camelcase" : 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb-base"
}
