{
 "db": {
   "name": "db",
   "connector": "memory",
   "file": "projectData.json"
 },
 "stage-db": {
   "name": "stage-db",
   "localStorage": "",
   "file": "projectData.json",
   "connector": "memory"
 },
 "stage-elastic": {
   "name": "stage-elastic",
   "connector": "rest",
   "debug": false,
   "crud": true,
   "operations": [
     {
       "template": {
         "method": "POST",
         "url": "http://54.173.14.8:9200/projects/_search",
         "headers": {
           "accepts": "application/json",
           "content-type": "application/json"
         },
         "body": {
           "query":{
             "multi_match" : {
               "query":      "{q}",
               "type":       "cross_fields",
               "fields":     ["project_name", "project_description"],
               "operator":   "or"
             }
           },
           "sort" : { "rank" : { "order" : "desc" }},
           "size" : 20
         }
       },
       "functions": {
         "search": [
           "q"
         ]
       }
     },

     {
       "template": {
         "method": "POST",
         "url": "http://54.173.14.8:9200/projects/_search",
         "headers": {
           "accepts": "application/json",
           "content-type": "application/json"
         },
         "body": {
           "query":{
             "multi_match" : {
               "query":      "{q}",
               "type":       "cross_fields",
               "fields":     ["project_name", "project_description","content"],
               "operator":   "or"
             }
           },
           "sort" : { "rank" : { "order" : "desc" }}
         }
       },
       "functions": {
         "searchAll": [
           "q"
         ]
       }
     }
   ]
 }
}
