{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "search-service": {
    "name": "search-service",
    "connector": "rest",
    "debug": false,
    "crud": true,
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "${ELASTICSEARCH_URL}/projects/_search",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "body": {
            "query": {
              "bool": {
                "should": [
                  {
                    "match": {
                      "_all": {
                        "query": "{params}",
                        "operator": "or",
                        "prefix_length": "1"
                      }
                    }
                  },
                  {
                    "match": {
                      "project_name": {
                        "query": "{params}",
                        "fuzziness": "2"
                      }
                    }
                  },
                  {
                    "match": {
                      "project_description": {
                        "query": "{params}"
                      }
                    }
                  },
                  {
                    "match": {
                      "readMe.content": {
                        "query": "{params}"
                      }
                    }
                  },
                  {
                    "match": {
                      "language": {
                        "query": "{params}"
                      }
                    }
                  },
                  {
                    "match_phrase": {
                      "project_name": {
                        "query": "{params}"
                      }
                    }
                  },
                  {
                    "match_phrase": {
                      "project_description": {
                        "query": "{params}"
                      }
                    }
                  },
                  {
                    "match_phrase": {
                      "readMe.content": {
                        "query": "{params}"
                      }
                    }
                  },
                  {
                    "match": {
                      "project_name.substring": {
                        "query": "{params}",
                        "boost": 0.3
                      }
                    }
                  }
                ]
              }
            },
            "highlight": {
              "pre_tags": [
                "<em class=\"search-highlight\">"
              ],
              "post_tags": [
                "</em>"
              ],
              "fields": {
                "readMe.content": {
                  "fragment_size": 1000,
                  "number_of_fragments": 5
                },
                "project_description": {
                  "fragment_size": 1000,
                  "number_of_fragments": 5
                },
                "project_name": {
                  "fragment_size": 250,
                  "number_of_fragments": 5
                }
              }
            },
            "size": "1000"
          }
        },
        "functions": {
          "search": [
            "params"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "${ELASTICSEARCH_URL}/projects/_doc/{id}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "query": {
            "x": "{x}"
          }
        },
        "functions": {
          "findById": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "POST",
          "url": "${ELASTICSEARCH_URL}/projects/_search",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "body": {
            "query": {
              "terms": {
                "_id": "{params:array}"
              }
            }
          }
        },
        "functions": {
          "findByIds": [
            "params"
          ]
        }
      },
      {
        "template": {
          "method": "POST",
          "url": "${ELASTICSEARCH_URL}/projects/_suggest",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "body": {
            "term-suggest": {
              "text": "{params}",
              "completion": {
                "field": "suggest",
                "size": 10
              }
            }
          }
        },
        "functions": {
          "findSuggestions": [
            "params"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "${ELASTICSEARCH_URL}/code/_doc/{id}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json",
            "User-Agent": ""
          },
          "query": {
            "x": "{x}"
          }
        },
        "functions": {
          "findComponentDependencies": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "${ELASTICSEARCH_URL}/projects/_search",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "body": {
            "size": 1000
          }
        },
        "functions": {
          "find": []
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "${ELASTICSEARCH_URL}/projects/_search",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "body": {
            "query": {
              "exists": {
                "field": "rank"
